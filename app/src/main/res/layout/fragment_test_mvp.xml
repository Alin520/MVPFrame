<?xml version="1.0" encoding="utf-8"?>
<com.alin.commonlibrary.view.RoundRectLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/test_rllyt"
    app:emptyView="@layout/layout_empty"
    app:errorView="@layout/layout_error"
    app:loadingView="@layout/layout_loading"
    android:orientation="vertical"
    android:background="@color/white"
    app:isShowContentView="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <TextView
        android:id="@+id/start_tv"
        android:layout_marginTop="30dp"
        android:text="点击测试MvpFragment数据通信"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/grey_all_a"
        />

    <TextView
        android:layout_marginTop="30dp"
        android:text="当前是MvpFragment测试页面"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/light_red"
        android:layout_gravity="center"
        />
</com.alin.commonlibrary.view.RoundRectLinearLayout>